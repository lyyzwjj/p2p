<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper
	namespace="cn.wolfcode.p2p.business.mapper.PlatformBankInfoMapper">
	<resultMap id="BaseResultMap"
		type="cn.wolfcode.p2p.business.domain.PlatformBankInfo">
		<id column="id" property="id" />
		<result column="bankName" property="bankName" />
		<result column="accountName" property="accountName" />
		<result column="accountNumber" property="accountNumber" />
		<result column="bankForkName" property="bankForkName" />
	</resultMap>
	<resultMap id="SmallResultMap"
		type="cn.wolfcode.p2p.business.domain.PlatformBankInfo">
		<id column="id" property="id" />
		<result column="bankName" property="bankName" />
		<result column="accountNumber" property="accountNumber" />
		<result column="bankForkName" property="bankForkName" />
	</resultMap>
	<delete id="deleteByPrimaryKey" parameterType="java.lang.Long">
		delete from
		platformbankinfo
		where id = #{id}
	</delete>
	<insert id="insert"
		parameterType="cn.wolfcode.p2p.business.domain.PlatformBankInfo"
		useGeneratedKeys="true" keyProperty="id">
		insert into platformbankinfo
		(bankName, accountName, accountNumber,
		bankForkName)
		values
		(#{bankName}, #{accountName}, #{accountNumber}, #{bankForkName})
	</insert>
	<update id="updateByPrimaryKey"
		parameterType="cn.wolfcode.p2p.business.domain.PlatformBankInfo">
		update platformbankinfo
		set bankName = #{bankName},
		accountName = #{accountName},
		accountNumber = #{accountNumber},
		bankForkName = #{bankForkName}
		where id = #{id}
	</update>
	<select id="selectByPrimaryKey" resultMap="BaseResultMap"
		parameterType="java.lang.Long">
		select id, bankName, accountName, accountNumber,
		bankForkName
		from platformbankinfo
		where id = #{id}
	</select>
	<select id="selectAll" resultMap="BaseResultMap">
		select id, bankName,
		accountName, accountNumber, bankForkName
		from platformbankinfo
	</select>
</mapper>